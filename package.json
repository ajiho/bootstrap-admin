{
  "name": "bootstrap-admin",
  "description": "Open source responsive background management template based on bootstrap 5.1.3+",
  "version": "2.0.1",
  "engines": {
    "node": "^18",
    "npm": ">=9"
  },
  "scripts": {
    "format": "prettier --config prettier.config.mjs  src/**/*.{js,scss} --check --cache --cache-location .cache/.prettiercache",
    "format:fix": "prettier --config prettier.config.mjs src/**/*.{js,scss} --write",
    "css:lint": "stylelint --config stylelint.config.mjs src/scss/**/*.scss --cache --cache-location .cache/.stylelintcache",
    "css:lint:fix": "stylelint --config stylelint.config.mjs src/scss/**/*.scss --fix",
    "css:format-lint": "run-s format css:lint",
    "css:compile": "sass --style expanded --source-map --embed-sources --no-error-css src/scss:dist/css",
    "css:prefix": "postcss --config postcss.config.mjs --replace \"dist/css/*.css\"  \"!dist/css*.min.css\"",
    "css:min": "cleancss -O1  --format breakWith=lf --with-rebase  --source-map --source-map-inline-sources --output dist/css/ --batch --batch-suffix \".min\" \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css:banner": "cross-env NODE_NO_WARNINGS=1 node build/banner-builder.mjs dist/css/*.css",
    "css:fix": "run-s css:lint:fix format:fix",
    "css": "run-s css:compile css:prefix css:min css:banner",
    "js": "shx mkdir -p dist/js/ && shx cp -r src/js/ dist/",
    "img": "shx mkdir -p dist/img/ && shx cp -r src/img/ dist/",
    "build": "run-s js css page",
    "lib": "node build/lib-builder.mjs",
    "geo": "node build/geo-data-builder.mjs",
    "mock": "cross-env NODE_NO_WARNINGS=1 node build/mock-builder.mjs",
    "page:compile": "cross-env NODE_NO_WARNINGS=1 node build/pages-builder.mjs src/pages/*.twig dist",
    "page": "run-s page:clean page:compile page:format",
    "docs:dev": "vitepress dev build/docs",
    "docs:build": "vitepress build build/docs",
    "docs:preview": "vitepress preview build/docs",
    "serve": "http-server dist -p 9999 -c-1",
    "clean": "run-p *:clean",
    "js:clean": "shx --silent rm -r dist/js",
    "css:clean": "shx --silent rm -r dist/css",
    "img:clean": "shx --silent rm -r dist/img",
    "lib:clean": "shx --silent rm -r dist/lib",
    "page:clean": "del-cli dist/*.html",
    "page:format": "js-beautify --type html --replace dist/*.html",
    "watch": "run-p watch-*",
    "watch-css": "nodemon --watch src/scss/ --ext scss --exec \"run-s css\"",
    "watch-pages": "nodemon --watch src/pages/ --ext twig --exec \"run-s pages\"",
    "watch-js": "nodemon --watch src/js/ --ext js --exec \"run-s js\""
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ajiho/bootstrap-admin.git"
  },
  "keywords": [
    "bootstrap",
    "bootstrap5",
    "css",
    "responsive",
    "admin",
    "template",
    "control-panel",
    "dashboard"
  ],
  "homepage": "https://gitee.com/ajiho/bootstrap-admin",
  "author": "ajiho",
  "license": "MIT",
  "devDependencies": {
    "@eonasdan/tempus-dominus": "^6.9.5",
    "@eslint/js": "^8.56.0",
    "@fonticonpicker/fonticonpicker": "^3.1.1",
    "@popperjs/core": "^2.11.8",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@uppy/locales": "^3.5.2",
    "autoprefixer": "^10.4.19",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "bootstrap-table": "^1.22.3",
    "browser-sync": "^3.0.2",
    "chart.js": "^4.4.2",
    "clean-css-cli": "^5.6.3",
    "countup.js": "^2.8.0",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "del-cli": "^5.1.0",
    "echarts": "^5.5.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "globals": "^13.24.0",
    "http-server": "^14.1.1",
    "jquery": "^3.7.1",
    "jquery-treegrid": "^0.3.0",
    "jquery.cookie": "^1.4.1",
    "js-beautify": "^1.15.1",
    "mockjs": "^1.1.0",
    "node-css-mqpacker": "^9.0.0",
    "nodemon": "^3.1.0",
    "npm-run-all2": "^6.1.2",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^2.6.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.2.5",
    "quicktab": "^0.0.2",
    "rollup": "^4.13.0",
    "sass": "^1.70.0",
    "shx": "^0.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "tua-body-scroll-lock": "^1.4.1",
    "twig": "^1.17.1",
    "uppy": "^3.23.0",
    "vitepress": "^1.0.0-rc.44",
    "vitepress-plugin-tabs": "^0.5.0"
  }
}
